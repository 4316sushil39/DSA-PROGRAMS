int* finalPrices(int* prices, int pricesSize, int* returnSize) {
    int i,j,flag = 0;
    for(i=0; i<priceSize; i++){
        for(j=i+1; j<prices[i]; j++){
           if(prices[j]<=prices[i]){
              prices[i] = prices[i]-prices[j];
            break;
    }   }
}
*returnSize = priceSize;
return prices;
}s
